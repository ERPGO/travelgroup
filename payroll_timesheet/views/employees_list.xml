<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="hr_account_ids_list_inherit1"
                model="ir.ui.view">
            <field name="name">Test Account id list inherit</field>
            <field name="model">hr.payslip</field>
            <field name="inherit_id" ref="hr_payroll.view_hr_payslip_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form[1]/sheet[1]/notebook[1]" position="inside">
                    <page name="test_account_ids" string="Analytic accounts">
                        <field name="account_ids">
                            <tree editable="bottom" string="Analytic accounts">
                                <field name="name"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>